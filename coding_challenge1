import matplotlib.pyplot as plt
import numpy as np

x = np.linspace(0, 2 * np.pi, 50)

plt.subplot(2,2,1)
plt.plot(x, np.sin(x), x, np.cos(x))

plt.subplot(2,2,2)
plt.scatter(x, np.exp(x))

plt.subplot(2,2,3)
plt.hist(np.random.rand(1000), 10)

plt.subplot(2,2,4)
plt.plot(np.random.rand(30), np.random.rand(30))

plt.show()
